{
  "name": "@donkeyclip/motorcortex-subtitles",
  "version": "3.0.4",
  "description": "Subtitles plugin for MotorCortex",
  "main": "dist/motorcortex-subtitles.cjs.js",
  "module": "dist/motorcortex-subtitles.esm.js",
  "browser": "dist/motorcortex-subtitles.umd.js",
  "author": "KissMyButton PC (kissmybutton.gr) <opensource@kissmybutton.gr>",
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeyclip/motorcortex-subtitles.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "start": "npm run build:lib && concurrently -c \"cyan.bold,magenta.bold\" \"npm:build:lib -- -w\"  \"npm:start:demo\" ",
    "start:demo": "webpack-dev-server --mode=development --config ./demo/webpack.config.js",
    "build:demo": "webpack --mode=production --config ./demo/webpack.config.js",
    "test": "HERE GOES YOUR TEST TASK",
    "test:prod": "npm run lint",
    "commit": "git-cz",
    "prebuild": "rimraf dist"
  },
  "keywords": [
    "motorcortex",
    "animation"
  ],
  "lint-staged": {
    "*.{json,css}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.15.8",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@donkeyclip/motorcortex": "7.5.5",
    "@donkeyclip/motorcortex-player": "^2.3.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@size-limit/preset-big-lib": "^6.0.3",
    "babel-loader": "^8.2.3",
    "browserslist": "^4.17.4",
    "caniuse-lite": "1.0.30001270",
    "commitizen": "4.2.4",
    "concurrently": "^6.3.0",
    "core-js": "^3.18.3",
    "css-loader": "^6.4.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "rollup-plugin-terser": "7.0.2",
    "size-limit": "^6.0.3",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "stream-browserify": "^3.0.0",
    "subtitle": "^4.1.1",
    "util": "^0.12.4"
  }
}
