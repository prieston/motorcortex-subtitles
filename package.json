{
  "name": "@donkeyclip/motorcortex-subtitles",
  "version": "3.0.4",
  "description": "Subtitles plugin for MotorCortex",
  "main": "dist/motorcortex-subtitles.cjs.js",
  "module": "dist/motorcortex-subtitles.esm.js",
  "browser": "dist/motorcortex-subtitles.umd.js",
  "author": "KissMyButton PC (kissmybutton.gr) <opensource@kissmybutton.gr>",
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeyclip/motorcortex-subtitles.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "start": "npm run build:lib && concurrently -c \"cyan.bold,magenta.bold\" \"npm:build:lib -- -w\"  \"npm:start:demo\" ",
    "start:demo": "webpack-dev-server --mode=development --config ./demo/webpack.config.js",
    "build:demo": "webpack --mode=production --config ./demo/webpack.config.js",
    "test": "HERE GOES YOUR TEST TASK",
    "test:prod": "npm run lint",
    "commit": "git-cz",
    "prebuild": "rimraf dist"
  },
  "keywords": [
    "motorcortex",
    "animation"
  ],
  "lint-staged": {
    "*.{json,css}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.15.0",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "7.15.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@donkeyclip/motorcortex": "^7.5.4",
    "@donkeyclip/motorcortex-player": "^2.3.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@size-limit/preset-big-lib": "^6.0.3",
    "babel-loader": "8.2.2",
    "browserslist": "4.16.7",
    "caniuse-lite": "1.0.30001270",
    "commitizen": "4.2.4",
    "concurrently": "^6.3.0",
    "core-js": "^3.18.3",
    "css-loader": "5.2.7",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-promise": "4.3.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "2.56.2",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "17.4.4",
    "size-limit": "^6.0.3",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "fs": "0.0.2",
    "monorepo": "^1.2.2",
    "subtitle": "^4.1.0"
  }
}
